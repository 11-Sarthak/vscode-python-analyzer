{
  "name": "python-analyzer",
  "version": "1.0.0",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "echo \"No lint script defined\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "vscode": "^1.75.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "python-analyzer.analyzeFunctions",
        "title": "Analyze Python Functions"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/vscode": "^1.101.0",
    "typescript": "^5.8.3"
  }
}